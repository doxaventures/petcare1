    <%=form_tag '/s', method: :get do%>
      <select class="form-control 1" id="user_category_id" name="category">
        <option>Choose a Pet</option>
        <%Category.all.each do |category|%>
          <option value="<%=category.url%>"><%=category.display_name(I18n.locale)%></option>
        <%end%>
      </select>
      <select class="form-control 2" id="user_category_id">
          <option value="0" selected>Choose a Service</option>
          <option value="1">Option 1</option>
          <option value="2">Option 2</option>
          <option value="3">Option 3</option>
